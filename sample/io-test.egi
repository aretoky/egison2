(define $main
  (lambda [$: $argv]
    (do {[$: (print : "input char : ")]
         [[$: $c] (read-char :)]
         [$: (write-string : "output : ")]
         [$: (write-char : c)]
         [$: (write-char : '\n')]
         [$: (print : "input number :")]
         [[$: $e] (read-line :)]
         [$: (write-string : "output : ")]
         [$: (print : e)]}
        :)))

(execute)



(define $main
  (lambda [$: $argv]
    (do {[$: (print : "Hello world!")]}
        :)))

(define $main
  (lambda [$: $argv]
    (do {[[$: $p] (open-output-file : "hello.txt")]
         [$: (print-to-port : p "Hello world!")]}
        :)))

(define $main
  (lambda [$: $argv]
    (do {[$: (write-string : "input char : ")]
         [[$: $c] (read-char :)]
         [$: (write-string : "output : ")]
         [$: (write-char : c)]
         [$: (write-char : '\n')]}
        :)))

(define $main
  (lambda [$: $argv]
    (do {[$: (write-string : "input : ")]
         [$: (flush :)]
         [[$: $str] (read-line :)]
         [$: (write-string : "output : ")]
         [$: (print : str)]}
        :)))

(define $main
  (lambda [$:]
    (do {[$: (print : "Return world? (y/n) : ")]
         [[$: $c] (read-char :)]}
        (match c Character
          {['y' :]
           ['n' 0]}))))

(execute)


(define $main
  (lambda [$:]
    (do {[$: (print : "Return world? (1/0) : ")]
         [[$: $n] (read :)]}
        (match n Integer
          {[,1 :]
           [,0 0]}))))

(execute)